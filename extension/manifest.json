{
  "manifest_version": 2,

  "name": "Defect Tracker",
  "description": "Screenshot saves into Rally",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/*",
    "https://rally1.rallydev.com/*"
  ]
}
